<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Multithreading" parallel="tests" thread-count="2">
    <test name="Chrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="org.example.AddItemAndCheckPrice"/>
        </classes>
    </test>
    <test name="Edge">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="org.example.AddItemAndCheckPrice"/>
        </classes>
    </test>
</suite>

